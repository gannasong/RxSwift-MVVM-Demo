# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'
use_frameworks!

target 'RxSwift+MVVM-Demo' do
  
  # Reactive
  pod 'RxSwift', '~> 5'
  pod 'RxCocoa', '~> 5'
  pod 'RxSwiftExt', '~> 5'
  pod 'RxDataSources', '~> 4.0'

  # Network
  pod 'Moya/RxSwift', '~> 14.0'

  # Image
  pod 'RxNuke', '~> 1.0'

  # Layout
  pod 'SnapKit', '~> 5.0.0'

  target 'RxSwift+MVVM-DemoTests' do
    inherit! :search_paths
    pod 'RxBlocking', '~> 5'
    pod 'RxTest', '~> 5'
  end

end
